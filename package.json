{
  "name": "local-npm-repository-manager",
  "displayName": "Local NPM Repository Manager",
  "description": "Manages a local cache of npm packages to avoid redundant downloads across projects",
  "publisher": "sublyime",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "npm",
    "package manager",
    "cache",
    "local repository",
    "development tools"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sublyime/local-npm-repository-manager.git"
  },
  "bugs": {
    "url": "https://github.com/sublyime/local-npm-repository-manager/issues"
  },
  "homepage": "https://github.com/sublyime/local-npm-repository-manager#readme",
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "local-npm-repository-manager.helloWorld",
        "title": "Hello World",
        "category": "Local NPM Repo"
      },
      {
        "command": "local-npm-repository-manager.installPackage",
        "title": "Install Package",
        "category": "Local NPM Repo"
      },
      {
        "command": "local-npm-repository-manager.checkUpdates",
        "title": "Check for Updates",
        "category": "Local NPM Repo"
      },
      {
        "command": "local-npm-repository-manager.showPackages",
        "title": "Show Local Packages",
        "category": "Local NPM Repo"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "local-npm-repository-manager.installPackage",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "local-npm-repository-manager.checkUpdates"
        },
        {
          "command": "local-npm-repository-manager.showPackages"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "esbuild": "^0.25.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
